<odoo>
    <record id="view_lc_management_type_tree" model="ir.ui.view">
        <field name="name">lc.type.tree</field>
        <field name="model">lc.type</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="action_lc_type" model="ir.actions.act_window">
        <field name="name">LC Type</field>
        <field name="res_model">lc.type</field>
        <field name="view_mode">list</field>
    </record>
</odoo>